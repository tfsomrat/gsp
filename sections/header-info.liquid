<!-- Script -->
<script>
  richSnippetReviewsWidgets('header-widget', {
    store: 'get-set-pet',
    starsClr: '#e75184',
    textClr: '#313131',
    logoClr: '#12cf6c',
    widgetName: 'text-banner',
    css: '.TextBanner>a>div{display:flex; align-items:center;} .TextBanner__text--customerssay{display:none} .TextBanner .TextBanner__text--verdict{display:none} .ReviewsLogo{display:none !important} .TextBanner .noBreakWrap{margin-top:0} .TextBanner__stars{margin-top:0 !important; padding:5px 5px; background-color: #e9f3fa; border-radius: 15px;} .TextBanner .TextBanner__stars.RatingStars--large i.RatingStars__star{font-size:17px}'
  });
</script>


<!-- Style -->
<style>
  .announcement-bar {
    padding: 15px 0;
    background: white;
    border-bottom: 1px solid #e6e6e6;
    height: 70px;
    overflow: hidden;
  }

  .header-info-slider a {
    text-decoration: none;
    color: #1C75BA;
  }
  .header-info-slider img,
  .header-info-slider p {
    display: inline-block !important;
    margin: 0;
  }

  .header-info-slider .slick-slide {
    text-align: center !important;
    line-height: 38px;
  }

  .header-info-slider p {
    color: #26536d;
    font-size: 16px;
    font-weight: 400;
    letter-spacing: 0.67px;
    line-height: 38px;
    padding-left: 10px;
    text-align: left;
    vertical-align: top;
  }

  @media(max-width: 1350px) {
    .header-info-slider p {
      font-size: 12px;
      letter-spacing: normal;
    }
  }
</style>

<script>
  $(document).ready(function() {
    $('.header-info-slider').slick({
      infinite: false,
      slidesToShow: 4,
      slidesToScroll: 1,
      fade: false,
      arrows: false,
      autoplay: false,
      autoplaySpeed: 3000,
      adaptiveHeight: false,
      lazyload: 'ondemand',
      responsive: [
        {
          breakpoint: 950,
          settings: {
            infinite: true,
            centerMode: false,
            slidesToShow: 2,
            slidesToScroll: 1,
            fade: false,
            arrows: false,
            autoplay: true,
            autoplaySpeed: 3000,
            adaptiveHeight: false
          }
        }, {
          breakpoint: 768,
          settings: {
            infinite: true,
            centerMode: false,
            slidesToShow: 1,
            slidesToScroll: 1,
            fade: false,
            arrows: false,
            autoplay: true,
            autoplaySpeed: 3000,
            adaptiveHeight: false
          }
        }
      ]
    });
  });
</script>

