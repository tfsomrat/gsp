<script>
	$(document).ready(function(){
		$('.has-submenu').click(function() {
			$(".has-submenu").not(this).removeClass('active');
			$(this).toggleClass('active');
		});

		$('.has-submenu ul').mouseleave(function(){
			$('.has-submenu').delay(300).removeClass('active');
		});

		$('.has-submenu ul').mouseleave(function(){
			$('.has-submenu').delay(300).removeClass('active');
		});

		$('.call-to-actions-slider').slick({
			infinite: false,
			slidesToShow: 4,
			slidesToScroll: 1,
			fade: false,
			arrows: false,
			autoplay: false,
			autoplaySpeed: 3000,
			adaptiveHeight: false,
			lazyload: 'ondemand',
			responsive: [
				{
					breakpoint: 950,
					settings: {
						infinite: true,
						centerMode: false,
						slidesToShow: 2,
						slidesToScroll: 1,
						fade: false,
						arrows: false,
						autoplay: true,
						autoplaySpeed: 3000,
						adaptiveHeight: false,
					}
				},
				{
					breakpoint: 768,
					settings: {
						infinite: true,
						centerMode: false,
						slidesToShow: 1,
						slidesToScroll: 1,
						fade: false,
						arrows: false,
						autoplay: true,
						autoplaySpeed: 3000,
						adaptiveHeight: false,
					}
				}
			]
		});


		$('.accordion-collect-tab').click(function(){
			if($(this).find('i').hasClass('active')) {
				$(this).find('i').removeClass('active');
				$(this).find('.accordion-collect-content').slideUp('fast');
			} else {
				$('.accordion-collect-toggle').find('i').removeClass('active');
				$('.accordion-collect-content').slideUp();
				$(this).find('i').toggleClass('active');
				$(this).find('.accordion-collect-content').slideToggle('fast');

			}
		});
	});
	</script>
