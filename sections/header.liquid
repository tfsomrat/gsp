<section data-section-id="{{ section.id }}" data-section-type="header-section">
	<header id="page-header" class="site-header d-none">
		<div class="container">
			<div class="row header-top">
				<!-- START Mobile Menu -->
				<div class="col-2 col-sm-4 align-self-center align-self-sm-center mobile-menu-button d-flex justify-content-center align-items-center d-sm-block d-md-block d-lg-none text-left">
					{% include 'icon-menu-icon' %}
				</div>
				<!-- END Mobile Menu -->
				<!-- START Logo -->
				<div class="col-8 col-sm-4 col-md-4 col-lg-4 col-xl-3 d-flex justify-content-center align-items-center align-self-centertext-center align-middle text-lg-left logo-wrapper">
					<a href="/">
						<img loading="lazy" class="header__logo d-none d-sm-inline-block d-md-inline-block d-lg-inline-block lazyload" data-src="{{ 'logo_png.png' | asset_img_url: '320x' }}" alt="{{ shop.name }}">
						<img loading="lazy" class="header__logo mobile_logo d-inline-block d-sm-none d-md-none d-lg-none lazyload" data-src="{{ 'mobile-logo_png.png' | asset_img_url: '200x' }}" alt="{{ shop.name }}">
					</a>
				</div>
				<!-- END Logo -->
				<!-- START Search -->
				<div class="d-none d-sm-none d-md-none d-lg-block col-lg-5 col-xl-6 text-center search-h align-self-center">
					<div class="header__search">
						{% include 'search' %}
					</div>
				</div>
				<!-- END Search -->
				<!-- START Account and Cart -->
				<div class="col-2 col-sm-4 col-md-4 col-lg-3 align-self-center">
						<ul class="header__cta--list d-flex d-md-block justify-content-end align-items-center">
							<li class="has-submenu">Help
								<ul style="width: 175px; left: -60px; top: 45px;">
									<div class="arrow-up"></div>
									<li  style="padding-top:5px;" class="intercom-click" onclick="ga('send', 'event', 'Help', 'Click', 'Chat');">Instant Chat</li>
									<li><a class="contact-us" href="/pages/contact-us" onclick="ga('send', 'event', 'Help', 'Click', 'Contact');">Contact Us</a></li>
									<li><a href="/pages/faqs#shipping" onclick="ga('send', 'event', 'Help', 'Click', 'Delivery');">Delivery Information</a></li>
									<li><a href="/pages/faqs" onclick="ga('send', 'event', 'Help', 'Click', 'FAQs');">FAQs</a></li>
								</ul>
							</li>
							<li class="has-submenu">Your Account
								<ul>
									<div class="arrow-up"></div>
									{% if customer %}
									<li><a style="padding-top:5px;" href="/account">Orders</a></li>
									<li><a href="/account/addresses">My Details</a></li>
									<li><a href="/account?view=my-pets">My Pets</a></li>
									<li><a href="/account/logout">Log Out</a></li>
									{% else %}
									<li><a href="/account">Sign In</a></li>
									<li><a href="/account/register">Register</a></li>
									{% endif %}
								</ul>
							</li>
							<li class="head-icon d-xs-inline-block d-lg-none p-0 cat-icon"><a class="p-0 d-flex justify-content-center align-items-center" href="/account">{% include 'icon-account-welcome' %}</a></li>
							{% assign has_collect = false %}
							{% for item in cart.items %}
								{% if item.product.title == 'Click & Collect' %}
									{% assign has_collect = true %}
								{% endif %}
							{% endfor %}
							{% if has_collect %}
								{% assign cart_amount = cart.items.size | minus: 1 %}
							{% else %}
								{% assign cart_amount = cart.items.size %}
							{% endif %}
							<li class="head-icon p-0"><a class="header__cart-price d-flex justify-content-center align-items-center" href="/cart" title="You have {{ cart_amount }} item(s) in your basket">{% include 'dynamic-cart-icon' %}<span class="item_count cart-count{% if cart_amount <= 0 %} hidden-count{% endif %}">{{ cart_amount }}</span></a></li>
						</ul>
				</div>
				<!-- END Account and Cart -->
				<div class="col-12 col-lg-12 d-none d-sm-none d-md-none d-lg-block mt-4">
					<div class="nav-holder">
						<ul class="site-nav ">
              				{% for link in linklists.main-menu-test.links %}
								<li class="top-level">
									<a class="top_level_a" href="{{ link.url }}">{{ link.title }}{% if link.links != blank %}{% include 'icon-chevron', fill_color: '#ffffff', direction: 'down' %}{% endif %}</a>
									{% assign toplevelink = link.url %}
									{% if link.links != blank %}
										<div class="dropdown" style="visibility: hidden;">
											<ul>
											{% for link in link.links limit: 3 %}
											<li id="{{ link.handle }}"><a href="{{ link.url }}">{{ link.title }}</a>
												{% if link.links != blank %}
													<ul class="test">
														{% for link in link.links limit: 11 %}
                                                          {% capture firstChar %}{{ link.title | slice: 0 }}{% endcapture %}
                                                          {% if firstChar == '|' %}
                                                            </ul>
                                                            <a href="{{ link.url }}">{{ link.title | remove_first: "|" }}</a>
                                                            <ul class="test">
                                                          {% else %}
                                                              <li id="{{ link.handle }}"><a href="{{ link.url }}">{{ link.title | remove_first: "|" }}</a></li>
            													{%- if forloop.index == 11 -%}
    																<li id="seemore"><a href="{{ toplevelink }}">See More</a></li>
                                                                {%- endif -%}                                                            
                                                          {% endif %}
														{% endfor %}
													</ul>
												{% endif %}
											</li>
											{% if forloop.last %}
												{% if forloop.length == 1 %}<li></li><li></li>{% endif %}
												{% if forloop.length == 2 %}<li></li>{% endif %}
											{% endif %}
											{% endfor %}
											
											<li style="text-align: center;">
												{% for block in section.blocks %}
													{%- if block.settings.page contains link.handle -%}
														<img loading="lazy" src="{{ block.settings.image | img_url: '500x' }}" alt="">
													{% endif %}
												{% endfor %}
											</li>
										</ul>
										</div>
									{% endif %}
								</li>
             	 			{% endfor %}
						</ul>
					</div>
				</div>

				<!-- START Search -->
				<div class="d-block d-sm-block d-md-block d-lg-none col-md-12 mt-4 text-center search-h align-self-center">
						<div class="header__search">
							{% include 'search' %}
						</div>
					</div>
					<!-- END Search -->
			</div>
		</div>
		<!-- {% form 'currency' %}
			<select name="currency">
				{% for currency in shop.enabled_currencies %}
				{% if currency == cart.currency %}
					<option selected="true">{{currency.iso_code}} {{currency.symbol}}</option>
					{% else %}
					<option>{{currency.iso_code}}</option>
				{% endif %}
				{% endfor %}
			</select>
		{% endform %} -->
	</header>
</section>
{% unless template == 'collection' %}
<section class="announcement-bar ">
	<div class="container">
		<div class="row">
			<div class="call-to-actions-slider">
				<div>
					<a style="text-decoration: none;  opacity: 1;" href="{{ section.settings.notification_link_one }}" onclick="ga('send', 'event', 'Header CTA', 'Click', 'Delivery info');">
						<img loading="lazy" style="position: relative; top: -1px;" data-lazy="{{ section.settings.notification_image_one | img_url: '30x' }}" alt="">
						<p>{{ section.settings.notification_text_one }}</p>
					</a>
				</div>
				<div>
					<a style="text-decoration: none; opacity: 1;" target="_blank" href="{{ section.settings.notification_link_two }}" onclick="ga('send', 'event', 'Header CTA', 'Click', 'Reviews');">
						<img loading="lazy" style="width: 100px; position: relative; top: -2px; " data-lazy="{{ section.settings.notification_image_two | img_url: 'master' }}" alt="">
						<p>{{ section.settings.notification_text_two }}</p>
					</a>
				</div>
				<div style="cursor: pointer;" data-toggle="modal" data-target="#exampleModal" onclick="ga('send', 'event', 'Header CTA', 'Click', 'Get Set Save');">
					<p style="line-height: 38px;">{{ section.settings.notification_text_three }}</p>
					<img loading="lazy" class="get-set-save-logo" style="width:90px; margin:0px 10px;" data-lazy="{{ 'getsetsave_colour_png.png' | asset_img_url: '90x' }}" alt="{{ section.settings.notification_text_three }}">
					<div class="learn-more">| Learn More</div>
				</div>
				{% if section.settings.notification_image_four != blank %}
					<div  style="cursor: pointer;" data-toggle="modal" data-target="#clickCollectModal" {% unless section.settings.notification_link_four %}onclick="ga('send', 'event', 'Header CTA', 'Click', '{{ section.settings.notification_text_four }}');"{% endunless %}>
						{% if section.settings.notification_link_four != blank %}
							<a style="text-decoration: none; opacity: 1;" target="_blank" href="{{ section.settings.notification_link_four }}" onclick="ga('send', 'event', 'Header CTA', 'Click', '{{ section.settings.notification_text_four }}');">
						{% endif %}
							{% if section.settings.notification_image_four != blank %}
								<img loading="lazy" style="width: 75px; position: relative; top: -2px; " data-lazy="{{ 'GSP_CC_png.png' | asset_img_url: '75x' }}" alt="">
							{% endif %}
							<div class="learn-more">| Learn More</div>
						{% if section.settings.notification_link_four != blank %}
							</a>
						{% endif %}
					</div>
				{% endif %}
			</div>
		</div>
	</div>
</section>
{% endunless %}
<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-body">
				<a href="/pages/faqs#GetSetSave">
					<img loading="lazy" style="cursor:pointer;" src="{{ section.settings.popup_image | img_url: 'master' }}" alt="Get Set Save">
				</a>
			</div>
		</div>
	</div>
</div>

<div class="modal fade" id="clickCollectModal" tabindex="-1" role="dialog" aria-labelledby="clickCollectModalLabel" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-body">
				<a href="/pages/faqs#clickCollect">
					<img loading="lazy" style="cursor:pointer;" src="{{ section.settings.click_collect_image | img_url: 'master' }}" alt="Get Set Save">
				</a>
			</div>
		</div>
	</div>
</div>
