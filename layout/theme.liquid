    {{
      '//cdn.shopify.com/s/files/1/0194/1736/6592/t/1/assets/booster-page-speed-optimizer.js?23'
      | script_tag
    }}
    {% render 'nosto-tagging' %}
    {% render 'klaviyo-add-to-cart' %}

    <!-- begin-boost-pfs-js -->
    {% render 'boost-pfs' %}
    <!-- end-boost-pfs-js -->
  </body>
</html>
