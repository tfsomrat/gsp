.header__search-box__predictive {
  position: absolute;
  right: 0;
  width: 100%;
  max-height: 500px;
  overflow-y: auto;
  z-index: 10;
  list-style: none;
  padding: 0;
  margin: 0;
  top: calc(100% + 5px);
}

.predictive-search {
  background-color: #fff;
  display: flex;
  flex-direction: column;
}

.predictive-search-order-1 {
  order: 1;
}

.predictive-search-order-2 {
  order: 2;
}

.predictive-search-order-3 {
  order: 3;
}

.predictive-search__list-item--search {
  order: 4;
}

.predictive-search__list-item {
  padding: 5px 10px;
}

.predictive-search__heading {
  text-align: left;
  padding: 5px 10px;
  font-size: 18px;
  margin: 0;
  background-color: #fafafa;
}

.predictive-search__item-content > span {
  display: block;
  text-align: left;
  color: #000;
}

.predictive-search__item-heading {
  line-height: 20px;
  font-size: 14px;
  color: inherit;
  padding-top: 0;
  padding-bottom: 6px;
  margin-top: 0;
  margin-bottom: 0;
  color: #000;
  text-transform: capitalize;
}

.predictive-search-order-1 .predictive-search__item-heading {
  font-weight: 600;
}

span.predictive-search__item-heading b {
  font-weight: 600;
}

.predictive-search__list-item .predictive-search__item-heading--price {
  color: #e35858;
}

.predictive-search__list-item a {
  text-decoration: none;
}

.caption-with-letter-spacing {
  text-transform: uppercase;
  background: #fafafa;
  padding: 6px 12px 5px;
  color: #2226;
  margin: 0;
  font-size: 14px;
  font-weight: 500;
  line-height: 20px;
}

.predictive-search__list-item:hover {
  background: #fafafa;
}

.predictive-search__item--term {
  appearance: none;
  width: 100%;
  border: none;
  background-color: #fafafa;
  display: block;
  text-align: center;
  font-size: 16px;
}

.predictive-search__overflow {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #222424;
  z-index: 5;
  pointer-events: none;
  opacity: 0;
}

.predictive-search__overflow.is-active {
  opacity: .65;
  pointer-events: all;
}

html.overflow-hidden,
.overflow-hidden body {
  overflow-y: hidden;
  height: 100%;
}

.boost-pfs-filter-grid-width-3 {
  width: 24.333333%;
}

.boost-pfs-search-suggestion-column-1:not(.boost-pfs-search-suggestion-mobile):not(.boost-pfs-search-suggestion-wrapper-style2).boost-pfs-search-suggestion-product-item-type-list .boost-pfs-search-suggestion {
