/*================ Rich Text Editor ================*/
.rte {
  font-size: 14px;
  line-height: 24px;
  font-weight: 300;
  line-height: 1.4;
  color: #000;

  h1 {
    @extend .title;
  }

  h2 {
    @extend .title-2;
  }

  h3 {
    @extend .section;
  }

  h4 {
    @extend .heading;
  }

  h5 {
    @extend .subheading;
  }

  h5 {
    @extend .detail;
  }

  a {
    text-decoration: none;
    color: $color-secondary;
  }

  img {
    height: auto;
  }

  table {
    table-layout: fixed;
  }

  ul,
  ol {
    margin: 0 0($gutter / 2);
  }

  b,
  strong {
    font-weight: bold;
  }

  // Match the styles from RTE nested lists
  ul {
    list-style: disc outside;

    li {
      margin-bottom: 10px;
      font-size: 14px;
    }

    ul {
      list-style: circle outside;

      ul {
        list-style: square outside;
      }
    }
  }
}

.text-center.rte,
.text-center .rte {
  ul,
  ol {
    margin-left: 0;
    list-style-position: inside;
  }
}



// allow table to scroll for tables in the RTE since we don't know
// how many columns they will contain. Class added by JS.
// sass-lint:disable no-misspelled-properties
.rte-table {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

// This class is wrapped around YouTube/Vimeo embeds in the RTE
// to make them responsive and prevent layout breaking
.rte__video-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 56.25%;
  height: 0;
  height: auto;

  iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

// This class is wrapped around tables in the RTE
// to prevent layout breaking with a scrollable parent
.rte__table-wrapper {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  font-size: 16px;
  line-height: 24px;
  font-weight: 400;
  color: #595959;
  margin-bottom: 0;

  a {
    color: $color-secondary;
    text-decoration: none;
    font-weight: 500;
  }
  table {
    border-bottom: 1px solid $color-border;
    tr {
      td {

         &:nth-of-type(1) {
          font-weight: 600;
          padding-right: 40px;
        }
      }
    }

    td {
      padding: 10px 0;
      border-top: 1px solid $color-border;
      vertical-align: top;

      p {
        margin: 0;
      }
    }
  }
